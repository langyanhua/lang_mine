create table if not exists www_user(
    uid bigint,
    username varchar(50),
    cell_phone bigint,
    password varchar(40),
    password_m varchar(6),
    kf_id bigint,
    group tinyint,
    reg_time bigint,
    login_time bigint,
    login_count bigint,
    online_time bigint,
    offline_ts bigint,
    reg_ip varchar(20),
    login_ip varchar(20),
    upgrade_start bigint,
    upgrade_end bigint,
    credit bigint,
    money double,
    charge_count bigint,
    channel_uid bigint,
    channel_sid bigint,
    openid varchar(32),
    token varchar(32),
    user_client_type tinyint,
    recommended tinyint,
    say_hi_recommended tinyint,
    isvip tinyint,
    vip_start bigint,
    vip_end bigint,
    ycoin bigint,
    real_reg_time timestamp,
    create_time timestamp,
    status tinyint,
    is_bind_roes tinyint
)
ROW FORMAT DELIMITED FIELDS TERMINATED BY '\1'
STORED AS ORC;
